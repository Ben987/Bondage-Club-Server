root = true

[*]
end_of_line = lf
charset = utf-8

[*.js]
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = true
indent_style = space

[*.json]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
